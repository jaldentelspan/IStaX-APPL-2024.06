    ::= { vtssRedboxCapabilitiesList 12 }

vtssRedboxCapabilitiesInterfacesTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSRedboxCapabilitiesInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table shows - per RedBox instance number - which interfaces can be
         configured on that instance"
    ::= { vtssRedboxCapabilities 2 }

vtssRedboxCapabilitiesInterfacesEntry OBJECT-TYPE
    SYNTAX      VTSSRedboxCapabilitiesInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each RedBox has a list of valid interfaces that can be used along with
         that instance"
    INDEX       { vtssRedboxCapabilitiesInterfacesInstance }
    ::= { vtssRedboxCapabilitiesInterfacesTable 1 }

VTSSRedboxCapabilitiesInterfacesEntry ::= SEQUENCE {
    vtssRedboxCapabilitiesInterfacesInstance    Unsigned32,
    vtssRedboxCapabilitiesInterfacesInterfaces  VTSSPortList
}

vtssRedboxCapabilitiesInterfacesInstance OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "RedBox instance number."
    ::= { vtssRedboxCapabilitiesInterfacesEntry 1 }

vtssRedboxCapabilitiesInterfacesInterfaces OBJECT-TYPE
    SYNTAX      VTSSPortList
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "List of valid ports for this RedBox instance."
    ::= { vtssRedboxCapabilitiesInterfacesEntry 2 }

vtssRedboxConfig OBJECT IDENTIFIER
    ::= { vtssRedboxMibObjects 2 }

vtssRedboxConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSRedboxConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "RedBox instance configuration table."
    ::= { vtssRedboxConfig 1 }

vtssRedboxConfigEntry OBJECT-TYPE
    SYNTAX      VTSSRedboxConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents a RedBox instance's configuration."
    INDEX       { vtssRedboxConfigInstance }
    ::= { vtssRedboxConfigTable 1 }

VTSSRedboxConfigEntry ::= SEQUENCE {
    vtssRedboxConfigInstance         Unsigned32,
    vtssRedboxConfigMode             VTSSRedBoxMode,
    vtssRedboxConfigPortA            VTSSInterfaceIndex,
    vtssRedboxConfigPortB            VTSSInterfaceIndex,
    vtssRedboxConfigNetId            VTSSUnsigned8,
    vtssRedboxConfigLanId            VTSSRedBoxLanId,
    vtssRedboxConfigNtAgeTime        VTSSUnsigned16,
    vtssRedboxConfigPntAgeTime       VTSSUnsigned16,
    vtssRedboxConfigDdAgeTime        VTSSUnsigned16,
    vtssRedboxConfigSvVlan           VTSSVlan,
    vtssRedboxConfigSvPcp            VTSSUnsigned8,
    vtssRedboxConfigSvDmacLsb        VTSSUnsigned8,
    vtssRedboxConfigSvFrameInterval  VTSSUnsigned16,
    vtssRedboxConfigSvXlatPrpToHsr   TruthValue,
    vtssRedboxConfigSvXlatHsrToPrp   TruthValue,
    vtssRedboxConfigAdminActive      TruthValue,
    vtssRedboxConfigAction           VTSSRowEditorState
}

vtssRedboxConfigInstance OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "RedBox instance number."
    ::= { vtssRedboxConfigEntry 1 }

vtssRedboxConfigMode OBJECT-TYPE
    SYNTAX      VTSSRedBoxMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Mode that this instance is running in."
