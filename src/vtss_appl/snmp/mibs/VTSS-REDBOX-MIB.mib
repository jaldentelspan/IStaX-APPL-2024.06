-- *****************************************************************
-- REDBOX-MIB:  Microsemi ENT private MIB
-- ****************************************************************

VTSS-REDBOX-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    vtssSwitchMgmt FROM VTSS-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSPortList FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    VTSSUnsigned64 FROM VTSS-TC
    VTSSUnsigned8 FROM VTSS-TC
    VTSSVlan FROM VTSS-TC
    ;

vtssRedboxMib MODULE-IDENTITY
    LAST-UPDATED "202309150000Z"
    ORGANIZATION
        "Microsemi Corporation"
    CONTACT-INFO
        "Application support: http://www.microchip.com/support"
    DESCRIPTION
        "Private MIB for IEC 62439-3 PRP/HSR RedBox."
    REVISION    "202309150000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 165 }


VTSSRedBoxLanId ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines whether the RedBox connects to LanA or LanB."
    SYNTAX      INTEGER { lanA(0), lanB(1) }

VTSSRedBoxMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the RedBox' mode of operation."
    SYNTAX      INTEGER { prpSan(0), hsrSan(1), hsrPrp(2), hsrHsr(3) }

VTSSRedBoxNodeType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Node type"
    SYNTAX      INTEGER { danP(0), danPRedBox(1), vDanP(2), danH(3),
                          danHRedBox(4), vDanH(5), san(6) }

VTSSRedBoxOperState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Operational state of a RedBox instance"
    SYNTAX      INTEGER { disabled(0), active(1), internalError(2) }

VTSSRedBoxSvType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Supervision frame type"
    SYNTAX      INTEGER { prpDd(0), prpDa(1), hsr(2) }

