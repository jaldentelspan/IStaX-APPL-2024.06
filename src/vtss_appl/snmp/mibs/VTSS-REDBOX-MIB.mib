vtssRedboxStatus OBJECT IDENTIFIER
    ::= { vtssRedboxMibObjects 3 }

vtssRedboxStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSRedboxStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Redbox status table."
    ::= { vtssRedboxStatus 1 }

vtssRedboxStatusEntry OBJECT-TYPE
    SYNTAX      VTSSRedboxStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents a RedBox instance's status."
    INDEX       { vtssRedboxStatusInstance }
    ::= { vtssRedboxStatusTable 1 }

VTSSRedboxStatusEntry ::= SEQUENCE {
    vtssRedboxStatusInstance                                    Unsigned32,
    vtssRedboxStatusOperState                                   VTSSRedBoxOperState,
    vtssRedboxStatusPortC                                       VTSSInterfaceIndex,
    vtssRedboxStatusNtPntFull                                   TruthValue,
    vtssRedboxStatusPortAWrongLan                               TruthValue,
    vtssRedboxStatusPortAHsrUntaggedRx                          TruthValue,
    vtssRedboxStatusPortADown                                   TruthValue,
    vtssRedboxStatusPortBWrongLan                               TruthValue,
    vtssRedboxStatusPortBHsrUntaggedRx                          TruthValue,
    vtssRedboxStatusPortBDown                                   TruthValue,
    vtssRedboxStatusPortCWrongLan                               TruthValue,
    vtssRedboxStatusPortCHsrUntaggedRx                          TruthValue,
    vtssRedboxStatusWarningNone                                 TruthValue,
    vtssRedboxStatusWarningMtuTooHighLrePorts                   TruthValue,
    vtssRedboxStatusWarningMtuTooHighNonLrePorts                TruthValue,
    vtssRedboxStatusWarningInterlinkNotCTagged                  TruthValue,
    vtssRedboxStatusWarningInterlinkNotMemberOfVlan             TruthValue,
    vtssRedboxStatusWarningNeighborRedBoxNotConfigured          TruthValue,
    vtssRedboxStatusWarningNeighborRedBoxNotActive              TruthValue,
    vtssRedboxStatusWarningNeighborRedBoxPortANotSetToNeighbor  TruthValue,
    vtssRedboxStatusWarningNeighborRedBoxPortBNotSetToNeighbor  TruthValue,
    vtssRedboxStatusWarningNeighborRedBoxOverlappingVlans       TruthValue,
    vtssRedboxStatusWarningStpEnabledInterlink                  TruthValue
}

vtssRedboxStatusInstance OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "RedBox instance number."
    ::= { vtssRedboxStatusEntry 1 }

vtssRedboxStatusOperState OBJECT-TYPE
    SYNTAX      VTSSRedBoxOperState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Operational state of this RedBox instance."
    ::= { vtssRedboxStatusEntry 2 }

vtssRedboxStatusPortC OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interface index of port C (the interlink port)."
    ::= { vtssRedboxStatusEntry 3 }

vtssRedboxStatusNtPntFull OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The combined NodesTable/ProxyNodeTable is full"
    ::= { vtssRedboxStatusEntry 4 }

vtssRedboxStatusPortAWrongLan OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port A has received traffic with wrong Lan ID"
    ::= { vtssRedboxStatusEntry 15 }

vtssRedboxStatusPortAHsrUntaggedRx OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port A has received traffic without HSR-tag from HSR ring"
    ::= { vtssRedboxStatusEntry 16 }

vtssRedboxStatusPortADown OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port A has link down"
    ::= { vtssRedboxStatusEntry 17 }

vtssRedboxStatusPortBWrongLan OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port B has received traffic with wrong Lan ID"
    ::= { vtssRedboxStatusEntry 28 }

vtssRedboxStatusPortBHsrUntaggedRx OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port B has received traffic without HSR-tag from HSR ring"
    ::= { vtssRedboxStatusEntry 29 }

vtssRedboxStatusPortBDown OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port B has link down"
    ::= { vtssRedboxStatusEntry 30 }

vtssRedboxStatusPortCWrongLan OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port C has received traffic with wrong Lan ID"
    ::= { vtssRedboxStatusEntry 41 }

vtssRedboxStatusPortCHsrUntaggedRx OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Port C has received traffic without HSR-tag from HSR ring"
    ::= { vtssRedboxStatusEntry 42 }

vtssRedboxStatusWarningNone OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "No configurational warnings found."
    ::= { vtssRedboxStatusEntry 54 }

vtssRedboxStatusWarningMtuTooHighLrePorts OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The MTU is too high on at least one of the LRE ports (max is 2000)"
    ::= { vtssRedboxStatusEntry 55 }

vtssRedboxStatusWarningMtuTooHighNonLrePorts OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The MTU is too high on at least one non-LRE port. Frames larger than
         1994 cannot traverse the HSR/PRP network"
    ::= { vtssRedboxStatusEntry 56 }

vtssRedboxStatusWarningInterlinkNotCTagged OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interlink port must use C-tags"
    ::= { vtssRedboxStatusEntry 57 }

vtssRedboxStatusWarningInterlinkNotMemberOfVlan OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interlink port is not member of the supervision frame VLAN ID"
    ::= { vtssRedboxStatusEntry 58 }

vtssRedboxStatusWarningNeighborRedBoxNotConfigured OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The neighbor RedBox is not configured"
    ::= { vtssRedboxStatusEntry 59 }

vtssRedboxStatusWarningNeighborRedBoxNotActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The neighbor RedBox is not active"
    ::= { vtssRedboxStatusEntry 60 }

vtssRedboxStatusWarningNeighborRedBoxPortANotSetToNeighbor OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The neighbor's port A is not configured as a RedBox neighbor"
    ::= { vtssRedboxStatusEntry 61 }

vtssRedboxStatusWarningNeighborRedBoxPortBNotSetToNeighbor OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The neighbor's port B is not configured as a RedBox neighbor"
    ::= { vtssRedboxStatusEntry 62 }

vtssRedboxStatusWarningNeighborRedBoxOverlappingVlans OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The neighbor's interlink port has coinciding VLAN memberships with this
