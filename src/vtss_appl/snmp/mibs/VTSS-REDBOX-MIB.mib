vtssRedboxCapabilitiesListDdAgeTimeMax OBJECT-TYPE
    SYNTAX      VTSSUnsigned16
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum allowed Duplicate Discard age time in milliseconds."
    ::= { vtssRedboxCapabilitiesList 8 }

vtssRedboxCapabilitiesListSvFrameIntervalMin OBJECT-TYPE
    SYNTAX      VTSSUnsigned16
    MAX-ACCESS  read-only
    STATUS      current
