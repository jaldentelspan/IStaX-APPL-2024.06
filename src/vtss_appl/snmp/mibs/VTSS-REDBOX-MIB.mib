    ::= { vtssRedboxStatusEntry 63 }

vtssRedboxStatusWarningStpEnabledInterlink OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interlink port has spanning tree enabled"
    ::= { vtssRedboxStatusEntry 64 }

vtssRedboxStatusNtTable OBJECT-TYPE
