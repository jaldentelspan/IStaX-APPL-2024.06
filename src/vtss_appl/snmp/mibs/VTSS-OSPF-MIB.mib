vtssOspfConfigRouterTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSOspfConfigRouterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is OSPF router configuration table. It is a general group to
         configure the OSPF common router parameters."
    ::= { vtssOspfConfig 3 }

vtssOspfConfigRouterEntry OBJECT-TYPE
    SYNTAX      VTSSOspfConfigRouterEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents the OSPF router interface
         configuration."
    INDEX       { vtssOspfConfigRouterInstanceId }
    ::= { vtssOspfConfigRouterTable 1 }

VTSSOspfConfigRouterEntry ::= SEQUENCE {
    vtssOspfConfigRouterInstanceId                          Integer32,
    vtssOspfConfigRouterIsSpecificRouterId                  TruthValue,
    vtssOspfConfigRouterRouterId                            IpAddress,
    vtssOspfConfigRouterDefPassiveInterface                 TruthValue,
    vtssOspfConfigRouterIsSpecificDefMetric                 TruthValue,
    vtssOspfConfigRouterDefMetricVal                        Unsigned32,
    vtssOspfConfigRouterConnectedRedistMetricType           VTSSOspfRedistributedMetricType,
    vtssOspfConfigRouterConnectedRedistIsSpecificMetric     TruthValue,
    vtssOspfConfigRouterConnectedRedistMetricVal            Unsigned32,
    vtssOspfConfigRouterStaticRedistMetricType              VTSSOspfRedistributedMetricType,
    vtssOspfConfigRouterStaticRedistIsSpecificMetric        TruthValue,
    vtssOspfConfigRouterStaticRedistMetricVal               Unsigned32,
    vtssOspfConfigRouterRipRedistMetricType                 VTSSOspfRedistributedMetricType,
    vtssOspfConfigRouterRipRedistIsSpecificMetric           TruthValue,
    vtssOspfConfigRouterRipRedistMetricVal                  Unsigned32,
    vtssOspfConfigRouterIsOnStartup                         TruthValue,
    vtssOspfConfigRouterOnStartupInterval                   Unsigned32,
    vtssOspfConfigRouterIsOnShutdown                        TruthValue,
    vtssOspfConfigRouterOnShutdownInterval                  Unsigned32,
    vtssOspfConfigRouterIsAdministrative                    TruthValue,
    vtssOspfConfigRouterDefaultRouteRedistMetricType        VTSSOspfRedistributedMetricType,
    vtssOspfConfigRouterDefaultRouteRedistIsSpecificMetric  TruthValue,
    vtssOspfConfigRouterDefaultRouteRedistMetricVal         Unsigned32,
    vtssOspfConfigRouterDefaultRouteRedistAlways            TruthValue,
    vtssOspfConfigRouterAdminDistance                       VTSSUnsigned8
}

vtssOspfConfigRouterInstanceId OBJECT-TYPE
    SYNTAX      Integer32 (1..1)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
