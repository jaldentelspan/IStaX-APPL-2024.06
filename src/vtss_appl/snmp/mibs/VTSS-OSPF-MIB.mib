vtssOspfConfig OBJECT IDENTIFIER
    ::= { vtssOspfMibObjects 2 }

vtssOspfConfigProcessTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSOspfConfigProcessEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is OSPF process configuration table. It is used to enable or
         disable the routing process on a specific process ID."
    ::= { vtssOspfConfig 1 }

vtssOspfConfigProcessEntry OBJECT-TYPE
    SYNTAX      VTSSOspfConfigProcessEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents OSPF routing process."
    INDEX       { vtssOspfConfigProcessInstanceId }
    ::= { vtssOspfConfigProcessTable 1 }

VTSSOspfConfigProcessEntry ::= SEQUENCE {
    vtssOspfConfigProcessInstanceId  Integer32,
    vtssOspfConfigProcessAction      VTSSRowEditorState
}

vtssOspfConfigProcessInstanceId OBJECT-TYPE
    SYNTAX      Integer32 (1..1)
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "The OSPF process instance ID."
    ::= { vtssOspfConfigProcessEntry 1 }

vtssOspfConfigProcessAction OBJECT-TYPE
    SYNTAX      VTSSRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { vtssOspfConfigProcessEntry 100 }

vtssOspfConfigProcessTableRowEditor OBJECT IDENTIFIER
