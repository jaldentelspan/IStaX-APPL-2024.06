VTSS-OSPF-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    vtssSwitchMgmt FROM VTSS-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    VTSSDisplayString FROM VTSS-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned8 FROM VTSS-TC
    ;

vtssOspfMib MODULE-IDENTITY
    LAST-UPDATED "202402090000Z"
    ORGANIZATION
        "Microsemi Corporation"
    CONTACT-INFO
        "Application support: http://www.microchip.com/support"
    DESCRIPTION
        "This is a private version of the OSPF MIB."
    REVISION    "202402090000Z"
    DESCRIPTION
        "Add MtuIgnore"
    REVISION    "201807090000Z"
    DESCRIPTION
        "Add RIP redistributed, database support, and correct the key length
         from 160 to 128 according to the cryptography."
    REVISION    "201805160000Z"
    DESCRIPTION
        "Update OSPF capabilities."
    REVISION    "201805140000Z"
    DESCRIPTION
        "Add stub router, NSSA, default route redistribution and passive
         interface support."
    REVISION    "201711110000Z"
    DESCRIPTION
        "Initial version."
    ::= { vtssSwitchMgmt 154 }


VTSSOspfAreaType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The area type."
    SYNTAX      INTEGER { normalArea(0), stubArea(1),
                          totallyStubArea(2), nssa(3), areaUnknown(4) }

VTSSOspfAuthType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates OSPF authentication type."
    SYNTAX      INTEGER { nullAuth(0), simplePasswordAuth(1),
                          md5Auth(2), areaConfigAuth(3) }

VTSSOspfBorderRouterType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The border router type of the OSPF route entry."
    SYNTAX      INTEGER { abr(0), asbrIntra(1), asbrInter(2),
                          abrAsbr(3), none(4) }

VTSSOspfInterfaceState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The state of the link."
    SYNTAX      INTEGER { down(1), loopback(2), waiting(3),
                          pointToPoint(4), drOther(5), bdr(6), dr(7),
                          unknown(8) }

VTSSOspfLsdbType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The type of the link state advertisement."
    SYNTAX      INTEGER { none(0), router(1), network(2), summary(3),
                          asbrSummary(4), external(5),
                          nssaExternal(7) }

VTSSOspfNeighborState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The state of the neighbor node."
    SYNTAX      INTEGER { dependupon(0), deleted(1), down(2),
                          attempt(3), init(4), twoWay(5), exstart(6),
                          exchange(7), loading(8), full(9),
                          unknown(10) }

VTSSOspfNssaTranslatorRole ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The OSPF NSSA translator role."
    SYNTAX      INTEGER { candidate(0), never(1), always(2) }

VTSSOspfNssaTranslatorState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The OSPF NSSA translator state."
    SYNTAX      INTEGER { disabled(0), elected(1), enabled(2) }

VTSSOspfRedistributedMetricType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The OSPF redistributed metric type."
    SYNTAX      INTEGER { metricTypeNone(0), metricType1(1),
                          metricType2(2) }

VTSSOspfRouteType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "The OSPF route type."
    SYNTAX      INTEGER { intraArea(0), interArea(1), borderRouter(2),
                          externalType1(3), externalType2(4),
                          unknown(5) }

