vtssIpConfigInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IPv4 interface configuration table. This table enables IPv4 related
         configuration of the corresponding IP interface."
    ::= { vtssIpConfigInterfaces 3 }

vtssIpConfigInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      VTSSIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { vtssIpConfigInterfacesIpv4IfIndex }
    ::= { vtssIpConfigInterfacesIpv4Table 1 }

VTSSIpConfigInterfacesIpv4Entry ::= SEQUENCE {
    vtssIpConfigInterfacesIpv4IfIndex                    VTSSInterfaceIndex,
    vtssIpConfigInterfacesIpv4Active                     TruthValue,
    vtssIpConfigInterfacesIpv4EnableDhcpClient           TruthValue,
    vtssIpConfigInterfacesIpv4Ipv4Address                IpAddress,
    vtssIpConfigInterfacesIpv4PrefixSize                 Unsigned32,
    vtssIpConfigInterfacesIpv4DhcpClientFallbackTimeout  Unsigned32,
    vtssIpConfigInterfacesIpv4DhcpClientHostname         VTSSDisplayString,
    vtssIpConfigInterfacesIpv4DhcpClientIdType           VTSSIpDhcpClientType,
    vtssIpConfigInterfacesIpv4DhcpClientIdIfMac          VTSSInterfaceIndex,
    vtssIpConfigInterfacesIpv4DhcpClientIdAscii          VTSSDisplayString,
    vtssIpConfigInterfacesIpv4DhcpClientIdHex            VTSSDisplayString
}

vtssIpConfigInterfacesIpv4IfIndex OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  accessible-for-notify
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { vtssIpConfigInterfacesIpv4Entry 1 }

vtssIpConfigInterfacesIpv4Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4.
         
         IPv4 can only be enabled if either the DHCP client is enabled, or a
         valid address has been configured."
    ::= { vtssIpConfigInterfacesIpv4Entry 2 }

vtssIpConfigInterfacesIpv4EnableDhcpClient OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4 DHCP client.
         
         Note: the DHCP client can only be enabled if there is no conflict in
         the values of: ipv4Address, prefixSize, and dhcpClientFallbackTimeout."
    ::= { vtssIpConfigInterfacesIpv4Entry 3 }

vtssIpConfigInterfacesIpv4Ipv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "IPv4 address.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { vtssIpConfigInterfacesIpv4Entry 4 }

vtssIpConfigInterfacesIpv4PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Prefix size of the network.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { vtssIpConfigInterfacesIpv4Entry 5 }

vtssIpConfigInterfacesIpv4DhcpClientFallbackTimeout OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "DHCP client fallback timer.
         
         If DHCP is disabled then this object has no effect. If DHCP is enabled
         and the fallback timeout value is different from zero, then this timer
         will stop the DHCP process and assign the ipv4Address to the interface
         instead."
    ::= { vtssIpConfigInterfacesIpv4Entry 6 }

vtssIpConfigInterfacesIpv4DhcpClientHostname OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..63))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The hostname of DHCP client. If DHCPv4 client is enabled, the
         configured hostname will be used in the DHCP option 12 field. When this
         value is empty string, the field use the configured system name plus
         the latest three bytes of system MAC addresses as the hostname."
    ::= { vtssIpConfigInterfacesIpv4Entry 7 }

vtssIpConfigInterfacesIpv4DhcpClientIdType OBJECT-TYPE
    SYNTAX      VTSSIpDhcpClientType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The type of the DHCP client identifier."
    ::= { vtssIpConfigInterfacesIpv4Entry 8 }

vtssIpConfigInterfacesIpv4DhcpClientIdIfMac OBJECT-TYPE
    SYNTAX      VTSSInterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The interface name of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type is 'ifmac', the configured
         interface's hardware MAC address will be used in the DHCP option 61
         field."
    ::= { vtssIpConfigInterfacesIpv4Entry 9 }

vtssIpConfigInterfacesIpv4DhcpClientIdAscii OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..31))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The ASCII string of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type is 'ascii', the ASCII string
         will be used in the DHCP option 61 field."
    ::= { vtssIpConfigInterfacesIpv4Entry 10 }

vtssIpConfigInterfacesIpv4DhcpClientIdHex OBJECT-TYPE
    SYNTAX      VTSSDisplayString (SIZE(0..64))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The hexadecimal string of DHCP client identifier. When DHCPv4 client is
         enabled and the client identifier type 'hex', the hexadecimal value
         will be used in the DHCP option 61 field."
    ::= { vtssIpConfigInterfacesIpv4Entry 11 }

