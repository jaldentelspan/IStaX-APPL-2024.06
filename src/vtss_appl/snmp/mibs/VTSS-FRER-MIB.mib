-- *****************************************************************
-- FRER-MIB:  Microsemi ENT private MIB
-- ****************************************************************

VTSS-FRER-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    vtssSwitchMgmt FROM VTSS-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    VTSSInterfaceIndex FROM VTSS-TC
    VTSSPortList FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    ;

vtssFrerMib MODULE-IDENTITY
    LAST-UPDATED "202402010000Z"
    ORGANIZATION
        "Microsemi Corporation"
    CONTACT-INFO
        "Application support: http://www.microchip.com/support"
    DESCRIPTION
        "Private MIB for Frame Replication and Elimination for Reliability,
         FRER."
    REVISION    "202402010000Z"
    DESCRIPTION
        "Added support for Stream Collections and pop of outer VLAN tag in
         generator mode"
    REVISION    "202311100000Z"
    DESCRIPTION
        "Added StreamIdMax to capabilities"
    REVISION    "202308290000Z"
    DESCRIPTION
        "Added number of ingress stream matches in generation mode"
    REVISION    "202212190000Z"
    DESCRIPTION
        "Added one more capability, indicating the maximum supported number of
         egress interfaces"
    REVISION    "202202230000Z"
    DESCRIPTION
        "PSFP and FRER can now use same stream IDs, causing one less operational
         warning"
    REVISION    "202009020000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 157 }


VTSSFrerFrerMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines whether FRER runs in generation or recovery
         mode."
    SYNTAX      INTEGER { generation(0), recovery(1) }

VTSSFrerOperState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Operational state of a FRER instance"
    SYNTAX      INTEGER { disabled(0), active(1), internalError(2) }

VTSSFrerRecoveryAlg ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the chosen recovery algorithm."
    SYNTAX      INTEGER { vector(0), match(1) }

