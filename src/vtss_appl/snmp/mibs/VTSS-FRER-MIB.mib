vtssFrerStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSFrerStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of created FRER instance status."
    ::= { vtssFrerStatus 1 }

vtssFrerStatusEntry OBJECT-TYPE
    SYNTAX      VTSSFrerStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a created FRER instance status."
    INDEX       { vtssFrerStatusId }
    ::= { vtssFrerStatusTable 1 }

VTSSFrerStatusEntry ::= SEQUENCE {
    vtssFrerStatusId                                                 Integer32,
    vtssFrerStatusOperState                                          VTSSFrerOperState,
    vtssFrerStatusLatentError                                        TruthValue,
    vtssFrerStatusWarningNone                                        TruthValue,
    vtssFrerStatusWarningStreamNotFound                              TruthValue,
    vtssFrerStatusWarningNoIngressPorts                              TruthValue,
    vtssFrerStatusWarningStreamAttachFail                            TruthValue,
    vtssFrerStatusWarningIngressEgressOverlap                        TruthValue,
    vtssFrerStatusWarningEgressPortCnt                               TruthValue,
    vtssFrerStatusWarningIngressNoLink                               TruthValue,
    vtssFrerStatusWarningEgressNoLink                                TruthValue,
    vtssFrerStatusWarningVlanMembership                              TruthValue,
    vtssFrerStatusWarningStpBlocked                                  TruthValue,
    vtssFrerStatusWarningMstpBlocked                                 TruthValue,
    vtssFrerStatusWarningStreamHasConfigurationalWarnings            TruthValue,
    vtssFrerStatusWarningStreamCollectionNotFound                    TruthValue,
    vtssFrerStatusWarningStreamCollectionAttachFail                  TruthValue,
    vtssFrerStatusWarningStreamCollectionHasConfigurationalWarnings  TruthValue
}

