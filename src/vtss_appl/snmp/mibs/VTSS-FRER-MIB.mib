VTSSFrerConfigEntry ::= SEQUENCE {
    vtssFrerConfigId                    Integer32,
    vtssFrerConfigMode                  VTSSFrerFrerMode,
    vtssFrerConfigFrerVlan              VTSSUnsigned16,
    vtssFrerConfigEgressPorts           VTSSPortList,
    vtssFrerConfigAlgorithm             VTSSFrerRecoveryAlg,
    vtssFrerConfigHistoryLen            Unsigned32,
    vtssFrerConfigResetTimeoutMsec      Unsigned32,
    vtssFrerConfigTakeNoSequence        TruthValue,
    vtssFrerConfigIndividualRecovery    TruthValue,
    vtssFrerConfigTerminate             TruthValue,
    vtssFrerConfigLaErrDetection        TruthValue,
    vtssFrerConfigLaErrDifference       Unsigned32,
    vtssFrerConfigLaErrPeriodMsec       Unsigned32,
    vtssFrerConfigLaErrPaths            Unsigned32,
    vtssFrerConfigLaErrResetPeriodMsec  Unsigned32,
    vtssFrerConfigAdminActive           TruthValue,
    vtssFrerConfigStreamId0             Unsigned32,
    vtssFrerConfigStreamId1             Unsigned32,
    vtssFrerConfigStreamId2             Unsigned32,
    vtssFrerConfigStreamId3             Unsigned32,
    vtssFrerConfigStreamId4             Unsigned32,
    vtssFrerConfigStreamId5             Unsigned32,
    vtssFrerConfigStreamId6             Unsigned32,
    vtssFrerConfigStreamId7             Unsigned32,
    vtssFrerConfigStreamCollectionId    Unsigned32,
    vtssFrerConfigIngressOuterTagPop    TruthValue,
    vtssFrerConfigAction                VTSSRowEditorState
}

