vtssStreamConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF VTSSStreamConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Stream configuration table."
    ::= { vtssStreamConfig 1 }

vtssStreamConfigEntry OBJECT-TYPE
    SYNTAX      VTSSStreamConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents a stream used for traffic
         filtering."
    INDEX       { vtssStreamConfigStreamId }
    ::= { vtssStreamConfigTable 1 }

VTSSStreamConfigEntry ::= SEQUENCE {
    vtssStreamConfigStreamId            Unsigned32,
    vtssStreamConfigDmacMatchType       VTSSStreamDmacMatchType,
    vtssStreamConfigDmac                MacAddress,
    vtssStreamConfigDmacMask            MacAddress,
    vtssStreamConfigSmac                MacAddress,
    vtssStreamConfigSmacMask            MacAddress,
    vtssStreamConfigOuterTag            VTSSStreamlanTagMatchType,
    vtssStreamConfigOuterTagTagType     VTSSStreamVlanTagType,
    vtssStreamConfigOuterTagVidValue    VTSSUnsigned16,
    vtssStreamConfigOuterTagVidMask     VTSSUnsigned16,
    vtssStreamConfigOuterTagPcpValue    VTSSUnsigned8,
    vtssStreamConfigOuterTagPcpMask     VTSSUnsigned8,
    vtssStreamConfigOuterTagDei         VTSSBitType,
    vtssStreamConfigInnerTag            VTSSStreamlanTagMatchType,
    vtssStreamConfigInnerTagTagType     VTSSStreamVlanTagType,
    vtssStreamConfigInnerTagVidValue    VTSSUnsigned16,
    vtssStreamConfigInnerTagVidMask     VTSSUnsigned16,
    vtssStreamConfigInnerTagPcpValue    VTSSUnsigned8,
    vtssStreamConfigInnerTagPcpMask     VTSSUnsigned8,
    vtssStreamConfigInnerTagDei         VTSSBitType,
    vtssStreamConfigProtocolType        VTSSStreamProtocolType,
    vtssStreamConfigEtherType           VTSSEtherType,
    vtssStreamConfigLlcDsap             VTSSUnsigned8,
    vtssStreamConfigLlcSsap             VTSSUnsigned8,
    vtssStreamConfigSnapOuiType         VTSSStreamSnapOuiType,
    vtssStreamConfigSnapOui             OCTET STRING,
    vtssStreamConfigSnapPid             VTSSUnsigned16,
    vtssStreamConfigIpv4SipAddress      IpAddress,
    vtssStreamConfigIpv4SipPrefixSize   Unsigned32,
    vtssStreamConfigIpv4DipAddress      IpAddress,
    vtssStreamConfigIpv4DipPrefixSize   Unsigned32,
    vtssStreamConfigIpv4DscpMatchType   VTSSStreamRangeMatchType,
    vtssStreamConfigIpv4DscpLow         VTSSUnsigned16,
    vtssStreamConfigIpv4DscpHigh        VTSSUnsigned16,
    vtssStreamConfigIpv4Fragment        VTSSBitType,
    vtssStreamConfigIpv4ProtoType       VTSSStreamIpProtocolType,
    vtssStreamConfigIpv4ProtoValue      VTSSUnsigned8,
    vtssStreamConfigIpv4DportMatchType  VTSSStreamRangeMatchType,
    vtssStreamConfigIpv4DportLow        VTSSUnsigned16,
    vtssStreamConfigIpv4DportHigh       VTSSUnsigned16,
    vtssStreamConfigIpv6SipAddress      InetAddressIPv6,
    vtssStreamConfigIpv6SipPrefixSize   Unsigned32,
    vtssStreamConfigIpv6DipAddress      InetAddressIPv6,
    vtssStreamConfigIpv6DipPrefixSize   Unsigned32,
    vtssStreamConfigIpv6DscpMatchType   VTSSStreamRangeMatchType,
    vtssStreamConfigIpv6DscpLow         VTSSUnsigned16,
    vtssStreamConfigIpv6DscpHigh        VTSSUnsigned16,
    vtssStreamConfigIpv6ProtoType       VTSSStreamIpProtocolType,
    vtssStreamConfigIpv6ProtoValue      VTSSUnsigned8,
    vtssStreamConfigIpv6DportMatchType  VTSSStreamRangeMatchType,
    vtssStreamConfigIpv6DportLow        VTSSUnsigned16,
    vtssStreamConfigIpv6DportHigh       VTSSUnsigned16,
    vtssStreamConfigPortList            VTSSPortList,
    vtssStreamConfigAction              VTSSRowEditorState
}

