VTSS-STREAM-MIB DEFINITIONS ::= BEGIN

IMPORTS
    NOTIFICATION-GROUP, MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    NOTIFICATION-TYPE, MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    vtssSwitchMgmt FROM VTSS-SMI
    InetAddressIPv6 FROM INET-ADDRESS-MIB
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    VTSSBitType FROM VTSS-TC
    VTSSEtherType FROM VTSS-TC
    VTSSPortList FROM VTSS-TC
    VTSSRowEditorState FROM VTSS-TC
    VTSSUnsigned16 FROM VTSS-TC
    VTSSUnsigned8 FROM VTSS-TC
    ;

vtssStreamMib MODULE-IDENTITY
    LAST-UPDATED "202401290000Z"
    ORGANIZATION
        "Microsemi Corporation"
    CONTACT-INFO
        "Application support: http://www.microchip.com/support"
    DESCRIPTION
        "Private MIB for Stream matching."
    REVISION    "202401290000Z"
    DESCRIPTION
        "Added stream collection support"
    REVISION    "202311230000Z"
    DESCRIPTION
        "Initial version"
    ::= { vtssSwitchMgmt 166 }


VTSSStreamDmacMatchType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "DMAC match type"
    SYNTAX      INTEGER { any(0), multicast(1), broadcast(2),
                          unicast(3), notBroadcast(4), notUnicast(5),
                          valueMask(6) }

VTSSStreamIpProtocolType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "IP protocol type"
    SYNTAX      INTEGER { any(0), custom(1), tcp(2), udp(3) }

VTSSStreamProtocolType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Protocol type"
    SYNTAX      INTEGER { any(0), etype(1), llc(2), snap(3), ipv4(4),
                          ipv6(5) }

VTSSStreamRangeMatchType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Range match type"
    SYNTAX      INTEGER { any(0), value(1), range(2) }

VTSSStreamSnapOuiType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "SNAP OUI type"
    SYNTAX      INTEGER { rfc1042(0), dot1h(1), custom(2) }

VTSSStreamVlanTagType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "VLAN Tag type"
    SYNTAX      INTEGER { any(0), cTagged(1), sTagged(2) }

VTSSStreamlanTagMatchType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "VLAN Tag match type"
    SYNTAX      INTEGER { both(0), untagged(1), tagged(2) }

vtssStreamMibObjects OBJECT IDENTIFIER
