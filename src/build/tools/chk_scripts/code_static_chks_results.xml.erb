<section name="" fontcolor="">
  <% @tables.each do |t| %>
  <table sorttable="yes">
    <tr>
      <% t.header.cells.each do |c| %>
      <td <%= c.as_xml %> />
      <% end %>
    </tr>

    <% t.rows.each do |r| %>
    <tr>
      <% r.cells.each do |c| %>
      <td <%= c.as_xml %> />
      <% end %>
    </tr>
    <% end %>
  </table>
  <% end %>
</section>
