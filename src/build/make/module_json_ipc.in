# Files Location
DIR_json_ipc := $(DIR_APPL)/json_ipc
MODULE_ID_json_ipc := 138 # VTSS_MODULE_ID_JSON_IPC

# Module objects
OBJECTS_json_ipc := json_ipc.o

# Compiler rules
$(OBJECTS_json_ipc): %.o: $(DIR_json_ipc)/%.cxx
	$(call compile_cxx,$(MODULE_ID_json_ipc), $@, $<)

# Include files
INCLUDES += -I$(DIR_json_ipc)

